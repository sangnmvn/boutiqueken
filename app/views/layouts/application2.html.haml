                .item
                  %a{:href => "#"}
                    %img.img-responsive{:alt => "slider-image", :src => "/assets/img/slider/slider-4.jpg"}/
              / .carousel-inner ends
              / Controls
              %a.left.carousel-control{"data-slide" => "prev", :href => "#slider-1", :role => "button"}
                %i.fa.fa-angle-left{"aria-hidden" => "true"}
                %span.sr-only Previous
              %a.right.carousel-control{"data-slide" => "next", :href => "#slider-1", :role => "button"}
                %i.fa.fa-angle-right{"aria-hidden" => "true"}
                %span.sr-only Next
