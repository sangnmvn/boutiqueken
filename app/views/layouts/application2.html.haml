          / TOP SLIDER AREA STARTS
          .section-slider-area
            %h2.sr-only Main Slider Area
            #slider-1.carousel.slide{"data-ride" => "carousel"}
              / Indicators
              %ol.carousel-indicators
                %li.active{"data-slide-to" => "0", "data-target" => "#slider-1"}
                %li{"data-slide-to" => "1", "data-target" => "#slider-1"}
                %li{"data-slide-to" => "2", "data-target" => "#slider-1"}
                %li{"data-slide-to" => "3", "data-target" => "#slider-1"}
              / Wrapper for slides
              .carousel-inner{:role => "listbox"}
                .item.active
                  %a{:href => "#"}
                    %img.img-responsive{:alt => "slider-image", :src => "/assets/img/slider/slider-1.jpg"}/
                .item
                  %a{:href => "#"}
                    %img.img-responsive{:alt => "slider-image", :src => "/assets/img/slider/slider-2.jpg"}/
                .item
                  %a{:href => "#"}
