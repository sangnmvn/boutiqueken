%aside
  %h2
    =@category.cat_name.upcase
  -group_name = ""
  - main_subs = @left_navs.group_by(&:group_name)
  -main_subs.keys.each do |key|
    .single-product-cat
      %strong.trigger
        =key.upcase
        %span
          %i.fa.fa-chevron-down
      %ul.ul-product-cat
        -main_subs[key].each do |i|
          -puts "----#{i.id}"
          %li
            %a{:href => category_path(i.category_id)}=i.cat_name
      